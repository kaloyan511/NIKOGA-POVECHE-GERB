int N = int.Parse(Console.ReadLine());
            int[] MYARRAY = new int[N];
            for (int i = 0;i < N; i++)
            {
                MYARRAY[i] = int.Parse(Console.ReadLine());
            }
            for (int i = 0; i < MYARRAY.Length - 1; i++)
            {
                if  ( Math.Abs(MYARRAY[i]) ==Math.Abs(MYARRAY[i + 1]))  
                {
                    Console.WriteLine($"[MYARRAY]");
                }
            }   
